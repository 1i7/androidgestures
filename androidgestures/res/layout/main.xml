<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:id="@+id/LinearLayout01"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:focusable="true" android:clickable="true"
	android:layout_height="fill_parent" android:layout_width="fill_parent">



	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_height="fill_parent" android:orientation="vertical"
		android:layout_width="wrap_content">
		<Button android:id="@+id/add_new" android:text="add new"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:focusable="false"></Button>
		<Button android:layout_height="wrap_content" android:id="@+id/delete"
			android:text="delete" android:layout_width="fill_parent"></Button>
		<Button android:layout_height="wrap_content" android:id="@+id/modify"
			android:text="modify" android:layout_width="fill_parent"></Button><Button android:layout_height="wrap_content" android:id="@+id/exit" android:text="exit" android:layout_width="fill_parent"></Button>
	
<Button android:layout_height="wrap_content" android:id="@+id/service_start" android:layout_width="fill_parent" android:text="start service"></Button>
</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout03"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
		<ListView android:id="@+id/ListView01" 
			
			
			android:choiceMode="singleChoice" android:layout_height="fill_parent" android:layout_width="fill_parent"></ListView>
	</LinearLayout>
</LinearLayout>
